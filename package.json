{
	"name": "{{name}}",
	"version": "{{version}}",
	"main": "lib/{{name}}.umd.js",
	"module": "lib/{{name}}.esm.js",
	"files": [
		"lib",
		"types",
		"packages"
	],
	"exports": {
    ".": {
      "import": "./dist/{{name}}.esm.js",
      "require": "./dist/{{name}}.umd.js"
    }
  },
	"types": "types/index.d.ts",
	"license": "MIT",
	"author": "{{author}}",
	"description": "{{description}}",
	"scripts": {
		"dev": "npm run clean && npm run lint && rollup -c config/rollup.config.js -w --environment NODE_ENV:development,IS_SERVE",
		"build": "npm run clean && npm run lint && rollup -c config/rollup.config.js --environment NODE_ENV:production",
		"build:components": "npm run clean:components && npm run lint && rollup -c config/rollup.config.js --environment NODE_ENV:production,BUILD:components",
		"build:bundle": "npm run clean:bundle && npm run lint && rollup -c config/rollup.config.js --environment NODE_ENV:production,BUILD:bundle",
		"clean": "rimraf ./lib ./dist ./coverage",
		"clean:test": "rimraf ./coverage",
		"clean:bundle": "rimraf ./lib/*.js ./lib/*.js.map",
		"clean:components": "rimraf ./lib/components ",
		"lint": "eslint --ignore-path .eslintignore .",
		"lint:fix": "eslint --fix --ignore-path .eslintignore .",
		"test": "npm run clean:test && jest"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@rollup/plugin-alias": "^3.1.8",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-image": "^2.1.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@types/html-minifier": "^4.0.1",
		"@types/jest": "^26.0.23",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"@vue/compiler-sfc": "^3.2.20",
		"@vue/test-utils": "^2.0.0-rc.6",
		"autoprefixer": "^9.8.8",
		"babel-jest": "^26.x",
		"esbuild": "^0.12.5",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^7.20.0",
		"jest": "26.6.3",
		"jest-serializer-vue": "^2.0.2",
		"jest-transform-stub": "^2.0.0",
		"node-sass": "^6.0.1",
		"postcss": "^8.3.11",
		"postcss-import": "^14.0.2",
		"postcss-minify": "^1.1.0",
		"postcss-preset-env": "^6.7.0",
		"pre-commit": "^1.2.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.59.0",
		"rollup-plugin-browsersync": "^1.3.3",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-esbuild": "^4.6.0",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-html2": "^3.1.0",
		"rollup-plugin-polyfill-node": "^0.7.0",
		"rollup-plugin-postcss": "^4.0.1",
		"rollup-plugin-progress": "^1.1.2",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-scss": "3",
		"rollup-plugin-vue": "^6.0.0",
		"ts-jest": "26.5.6",
		"typescript": "^4.4.4",
		"vue": "^3.2.20",
		"vue-jest": "^5.0.0-alpha.10"
	},
	"pre-commit": [
		"lint"
	]
}
